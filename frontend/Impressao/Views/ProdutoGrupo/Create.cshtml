@model Impressao.Models.ProdutoGrupo

@{
    ViewData["Title"] = "Grupo de produtos";
}

<div class="content-header">
    <div class="container-fluid">
        <div class="row mb-2">
            <div class="col-sm-6">
                <h1 class="m-0">Grupo de produtos</h1>
            </div>
            <div class="col-sm-6">
                <ol class="breadcrumb float-sm-right">
                    <li class="breadcrumb-item"><a asp-controller="Home" asp-action="Index">Home</a></li>
                    <li class="breadcrumb-item"><a asp-action="Index">Grupos de produtos</a></li>
                    <li class="breadcrumb-item active">Grupo</li>
                </ol>
            </div>
        </div>
    </div>
</div>

<section class="content">
    <div class="container-fluid">
        <div class="card">
            <div class="card-body">
                <form asp-action="Create" method="post">
                    @Html.Partial("~/Views/ProdutoGrupo/Form.cshtml")
                </form>
            </div>
        </div>
    </div>
</section>

<script src="~/js/produtoGrupo/edit.js"></script>